{
    "Requests": 
    [
        {
            "Title": "--In this example, send multiple requests for what you want done.--",

            "TargetURL": "www.google.com",
            "HTTP_Method": "GET",
            "HTTP_Method_options": "POST,GET,PUT,DELETE",
            "Headers": [
                {
                    "Name": "Content-Type",
                    "Value": "application/html"
                },
                { "Content-Type_options1": "application/json,application/pdf" },
                { "Content-Type_options2": "application/x-www-form-urlencoded,text/xml" },
                { "Content-Type_options3": "text/html,audio/mpeg,video/mp4,image/jpeg" },

                {
                    "Name": "Header_key1",
                    "Value": "Header_value"
                },
                {
                    "Name": "Header_key2",
                    "Value": "Header_value"
                },
                {
                    "Name": "Accept",
                    "Value": "*/*"
                }
            ],
            "Request_Body": "",
            "Request_Body_FileName": "",

            "Follow_Redirects": true,

            "ReturnType": "JSON",
            "ReturnType_options": "JSON,ToFile,None",


            "Output_Filename": ".\\response.txt",

            "EndNote": "---------------------------------------"
        },
